#include <stdio.h>

int main() {

    char name;
    int age;

    printf("\n  Name:  ");
    scanf("%[^\n]*c",&name);


    printf("\n  Age:  ");
    int count = scanf(" %d", &age);


    if (count != 1)
{
     printf("\n  Invalid Age, Please Try Again.\n");
}


    else if (age >= 18 && age < 100)
    {
        printf("\n  You are eligible for voting.\n");
    }

    else
    {
        printf("\n  You are not eligible for voting.\n");
    }

   return 0;
}
